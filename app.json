{
    "expo": {
        "name": "blockchain explorer",
        "description": "Scan QR code and read the message \nstored in the blockchain transaction.",
        "slug": "blockchain",
        "privacy": "public",
        "sdkVersion": "30.0.0",
        "platforms": ["ios", "android"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "primaryColor": "#000000",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        }
    }
}
